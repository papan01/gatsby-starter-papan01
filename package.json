{
  "name": "papan-gatsby-starter",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{json,md,scss}\"",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@loadable/component": "^5.11.0",
    "classnames": "^2.2.6",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.18.11",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-feed": "^2.3.20",
    "gatsby-plugin-google-analytics": "^2.1.28",
    "gatsby-plugin-manifest": "^2.2.26",
    "gatsby-plugin-netlify-cms": "^4.1.33",
    "gatsby-plugin-offline": "^3.0.18",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.20",
    "gatsby-plugin-sharp": "^2.2.37",
    "gatsby-plugin-sitemap": "^2.2.20",
    "gatsby-remark-autolink-headers": "^2.1.18",
    "gatsby-remark-custom-blocks": "^2.1.21",
    "gatsby-remark-images": "^3.1.31",
    "gatsby-remark-prismjs": "^3.3.23",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.3.3",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.13.1",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/papan01/blog"
  },
  "bugs": {
    "url": "https://github.com/papan01/blog/issues"
  }
}
